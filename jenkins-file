pipeline {
    agent any

    stages {
        stage('check-status') {
            steps {
             build quietPeriod: 5, job: 'Git status'
            }
        }
        stage('Show file content') {
            steps {
             build quietPeriod: 5, job: 'Git pull'
            }
        }
        stage('Git Push') {
            steps {
             build quietPeriod: 5, job: 'Git push'
            }
        }
    }
}
